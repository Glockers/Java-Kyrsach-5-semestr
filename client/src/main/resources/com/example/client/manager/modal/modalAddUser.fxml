<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="416.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.controller.manager.modal.UserModalController">
    <children>
        <Label layoutX="128.0" layoutY="22.0" prefHeight="54.0" prefWidth="510.0" text="Добавление пользователя">
            <font>
                <Font size="37.0" />
            </font>
        </Label>
        <VBox layoutX="128.0" layoutY="105.0" prefHeight="90.0" prefWidth="472.0">
            <children>
                <TextField fx:id="fieldLogin" prefHeight="25.0" prefWidth="365.0" promptText="Придумайте Логин">
                    <VBox.margin>
                        <Insets bottom="35.0" />
                    </VBox.margin>
                </TextField>
            <PasswordField fx:id="passwordField" promptText="Придумайте пароль" />
                <HBox prefHeight="1.0" prefWidth="472.0">
                    <VBox.margin>
                        <Insets bottom="30.0" />
                    </VBox.margin>
                </HBox>
                <HBox prefHeight="0.0" prefWidth="472.0">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
        <Button fx:id="cancelBtn" layoutX="438.0" layoutY="322.0" mnemonicParsing="false" onAction="#onCancelClick" prefHeight="54.0" prefWidth="162.0" text="Отменить" />
        <Button fx:id="saveBtn" layoutX="128.0" layoutY="322.0" mnemonicParsing="false" onAction="#onSaveClick" prefHeight="54.0" prefWidth="162.0" text="Зарегистрировать" />
        <ComboBox fx:id="comboBox" layoutX="414.0" layoutY="220.0" prefHeight="25.0" prefWidth="186.0" />
        <Label layoutX="128.0" layoutY="224.0" prefHeight="17.0" prefWidth="162.0" text="Устновите роль" />
    </children>
</AnchorPane>
